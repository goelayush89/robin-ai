{
  "name": "@robin/core",
  "version": "0.1.0",
  "private": true,
  "description": "Core business logic and types for Robin Assistant",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.mjs",
      "require": "./dist/agents/index.js"
    },
    "./operators": {
      "types": "./dist/operators/index.d.ts",
      "import": "./dist/operators/index.mjs",
      "require": "./dist/operators/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.mjs",
      "require": "./dist/models/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@robin/tsconfig": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist",
    "src"
  ]
}
